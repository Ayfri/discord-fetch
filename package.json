{
	"description": "A npm package for fetching messages from a Discord Guild or a Discord Channel.",
	"devDependencies": {
		"@types/node": "^14.14.35",
		"prettier": "^2.2.1",
		"typescript": "^4.2.3"
	},
	"dependencies": {
		"discord.js": "^12.5.1"
	},
	"engineStrict": true,
	"engines": {
		"node": ">=14.0.0",
		"discord.js": ">=12.0.0"
	},
	"keywords": [
		"discord.js",
		"discord-messages",
		"discord-bot",
		"fetch"
	],
	"main": "index.js",
	"name": "discord-fetch-messages",
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
		"endOfLine": "lf",
		"printWidth": 200,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	},
	"types": "types",
	"readme": "https://github.com/Ayfri/discord-fetch/blob/main/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ayfri/discord-fetch"
	},
	"scripts": {
		"build": "npm run prettier && tsc",
		"prepare": "npm run build",
		"prettier": "prettier . --write"
	},
	"version": "2.1.0"
}
