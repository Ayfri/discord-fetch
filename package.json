{
	"author": "Ayfri",
	"bugs": {
		"url": "https://github.com/Ayfri/discord-fetch/issues"
	},
	"dependencies": {
		"discord.js": "^13.12.0"
	},
	"description": "A npm package for fetching messages from a Discord Guild, Guild list, Channel, Channel list, Thread or Thread list.",
	"devDependencies": {
		"@types/node": "^16.11.66",
		"prettier": "^2.7.1",
		"typescript": "^5.1.3"
	},
	"engineStrict": true,
	"engines": {
		"node": ">=16.9.0",
		"discord.js": ">=13.7.0"
	},
	"keywords": [
		"discord.js",
		"discord-messages",
		"discord-bot",
		"fetch",
		"fetch-messages",
		"discord-fetch"
	],
	"license": "GNU",
	"main": "dist",
	"module": "dist/index.js",
	"name": "discord-fetch-messages",
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
		"printWidth": 200,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	},
	"readme": "https://github.com/Ayfri/discord-fetch/blob/main/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ayfri/discord-fetch-messages.git"
	},
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "npm run prettier && npm run build",
		"prettier": "prettier src *.json *.md --write"
	},
	"types": "dist",
	"version": "2.5.0"
}
